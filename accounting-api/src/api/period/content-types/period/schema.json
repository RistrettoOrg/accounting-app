  },
  "attributes": {
    "start_date": {
      "type": "date",
      "required": true
    },
    "end_date": {
      "type": "date",
      "required": true
    },
    "status_period": {
      "type": "enumeration",
      "enum": [
        "open",
        "closed"
      ]
    },
    "uuid": {
      "type": "uid",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
